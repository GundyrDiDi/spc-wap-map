    "lodash": "^4.17.15",
    "mockjs": "^1.1.0",
    "ol": "^6.2.1",
    "swiper": "^5.3.6",
    "vconsole": "^3.3.4",
    "velocity-animate": "^1.5.2",
    "vue": "^2.6.11",
    "vue-lottie": "^0.2.1",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
